{
  "manifest_version": 2,
  "name": "Web tracking analyser",
  "author": "Lewis Luckhurst",
  "version": "1.0",
  "description": "Used to analyser various tracked information online",
  "content_security_policy": "script-src 'self' https://localhost:8080/map; object-src 'self'",
  "browser_action": {
    "default_title": "Open the popup"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "activeTab",
    "*://*/*",
    "tabs"
  ]
}
